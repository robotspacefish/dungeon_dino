function upd_reset_game()
	clear_table(game_objects)
	--reset original map sprites
	for room in all(rooms) do
		for t in all(room.layout) do
			mset(t.x,t.y,t.spr)
		end
	end
	clear_table(current_room)
	start_game()
end
