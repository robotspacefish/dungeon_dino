--===== collision functions ======================================
function has_flag(next_tile,flag)
	if fget(next_tile,flag) then return true end
	return false
end

function is_item_collision(next_x,next_y)
	for obj in all(game_objects) do
		if (next_x==obj.x and next_y==obj.y) return obj
	end
	return nil
end

function handle_item_collision(obj)

	if obj.name=="vase" then
		local v_spr=obj.v_spr
		local smash_spr
		if v_spr==lg_vase_spr[1] then
			smash_spr=lg_vase_spr[4]
			sfx(1)
		else
			smash_spr=sm_vase_spr[4]
			sfx(2)
		end

		current_room.vases_left-=1
		current_room.vases_smashed+=1
		obj.is_smashed=true --todo for future use?
		mset(obj.x,obj.y,smash_spr) --todo lg & sm

		--check vase for bomb
		if obj.has_bomb then
			--explode bomb
				create_bomb(obj.x,obj.y)
			 player.health-=1
				player.hit=true
			end

		-- if (player.health==0) mode="game_over" --todo put back

		--check vase for gem
		if (obj.has_gem) then
			--todo gem animation

			player.gems+=1
			current_room.gems_collected+=1
			-- current_room.gems-=1
		end
	end

	if obj.name=="potion" then
		player:heal(true)
		mset(obj.x,obj.y,floor_spr[1])
	end
		del(game_objects, obj)
end
